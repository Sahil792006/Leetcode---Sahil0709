#using array 
class Solution {
    public int findDuplicate(int[] nums) {
        int seen[] = new int[nums.length];
        for(int i : nums){
            if(seen[i-1] == 1){
                return i;
            }
            seen[i-1] = 1;
        }
        return -1;
    }
}

#fast slow method(Floyd's Cycle-Finding Algorithm)
class Solution {
    public int findDuplicate(int[] nums) {
        int fast = nums[0];
        int slow = nums[0];
        do{
            fast = nums[nums[fast]];
            slow = nums[slow];
        }while(slow != fast);
        slow = nums[0];
        while(slow != fast){
            fast = nums[fast];
            slow = nums[slow];
        }
        return slow;
    }
}
