class Solution {
    public int evalRPN(String[] tokens) {
        Stack<Integer> s = new Stack<>();
        for(String t : tokens){
            if(t.equals("+") || t.equals("-") || t.equals("/") || t.equals("*"))
            {
                int b = s.pop();
                int a = s.pop();
                int ans = 0;
                switch(t){
                    case "+" : ans = (a + b) ; break;
                    case "-" : ans = (a - b) ; break;
                    case "/" : ans = (a / b) ; break;
                    case "*" : ans = (a * b) ; break;
                }
                s.push(ans);
            }
            else {
                s.push(Integer.parseInt(t));
            }
        }
        return s.pop();
    }
}
